{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "comma-dangle": "off",
    "no-await-in-loop": "off",
    "no-console": "off",
    "no-restricted-syntax": "off",
    "linebreak-style": "off",
    "operator-linebreak": "after"
  }
}
